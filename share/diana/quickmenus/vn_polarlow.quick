OBJECTS NAME="VNN Bakkeanalyse" types=front,symbol,area,anno timediff=180 alpha=0.3
MAP map=Gshhs-Auto contour=on cont.colour=landgul2 cont.linewidth=1 cont.linetype=solid cont.zorder=1 land=on land.colour=lightgray land.zorder=0 latlon=off frame=off
MAP map=Euro2 contour=on cont.colour=lightGray cont.linewidth=1 cont.linetype=solid cont.zorder=1 land=off latlon=off frame=off
