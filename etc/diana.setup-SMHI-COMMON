languagepaths=$(DIANADIR)/share/diana/lang:lang:../lang:$(ROOT)/translations:${QTDIR}/translations
setenv=BUFR_TABLES,/usr/local/lib/bufrtables/,1
language=no
avatars=${HOME}/.diana/avatars/*.png
</BASIC>


