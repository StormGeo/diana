mslp=multiply(mslp_pa,0.01)
thunderprob_percent=multiply(thunderprob,100)
th_celsius=subtract(th,273.15)
rh_percent=multiply(rh,100)

# Ugly way
u=multiply(u10m,1)
v=multiply(v10m,1)
